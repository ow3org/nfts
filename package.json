{
  "name": "web-components-library-starter",
  "version": "0.3.0",
  "packageManager": "pnpm@7.6.0",
  "description": "The easiest way to ensure your media/data is always displayed quickly, securely and at the highest quality possible.",
  "keywords": [
    "web-components",
    "component",
    "library",
    "vue",
    "typescript",
    "javascript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/meemalabs/web-components-library-starter.git"
  },
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vite build",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "vitebook:dev": "vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:e2e": "cypress open",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "8.9.4",
    "vue": "3.2.37"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@babel/types": "7.18.9",
    "@vitebook/client": "0.100.5",
    "@vitebook/core": "0.100.5",
    "@vitebook/markdown-vue": "0.100.5",
    "@vitebook/theme-default": "0.100.5",
    "@vitebook/vue": "0.100.5",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/compiler-sfc": "3.2.37",
    "autoprefixer": "10.4.7",
    "cypress": "9.7.0",
    "eslint": "8.20.0",
    "eslint-plugin-cypress": "2.12.1",
    "https-localhost": "4.7.1",
    "pnpm": "7.6.0",
    "postcss": "8.4.14",
    "postcss-import": "14.1.0",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.12",
    "svelte": "3.49.0",
    "tailwindcss": "3.1.6",
    "typescript": "4.7.4",
    "unplugin-auto-import": "0.10.0",
    "unplugin-vue-components": "0.21.1",
    "vite": "2.9.14",
    "vitest": "0.18.1",
    "vue-tsc": "0.39.0"
  },
  "engines": {
    "node": ">=16.*"
  },
  "contributors": [
    "Chris Breuer <chris@meema.xyz>"
  ],
  "bugs": {
    "url": "https://github.com/meemalabs/web-components-library-starter/issues"
